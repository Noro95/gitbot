{
	"compilerOptions": {
		"target": "ESNext",
		"module": "NodeNext",
		"rootDir": "src",
		"moduleResolution": "NodeNext",
		"allowJs": false,
		"outDir": "dist",
		"removeComments": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"include": ["src"]
}
